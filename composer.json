{
    "name": "rickycezar/laravel-jwt-impersonate",
    "description": "Laravel Impersonate is a plugin that allows to you to authenticate as your users.",
    "type": "library",
    "keywords": [
      "laravel",
      "package",
      "plugin",
      "impersonate",
      "impersonation",
      "user",
      "auth",
      "laravel-package",
      "laravel-plugin"
    ],
    "require": {
      "php": "^7.1.3 || ^8.0",
      "laravel/framework": ">=5.8",
      "tymon/jwt-auth": "*",
       "composer/installers": "~1.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^7.5 || ^8.0",
      "orchestra/testbench": "~3.8.0",
      "orchestra/database": "~3.8.0"
    },
    "license": "MIT",
    "authors": [
      {
        "name": "rickycezar",
        "email": "barros.henrique.c@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Rickycezar\\Impersonate\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Rickycezar\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "installer-name": "Rickycezar"
    }
}
