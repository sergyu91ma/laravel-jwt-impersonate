{
    "name": "rickycezar/laravel-jwt-impersonate",
    "description": "Laravel Impersonate is a plugin that allows to you to authenticate as your users.",
    "type": "library",
    "keywords": [
      "laravel",
      "package",
      "plugin",
      "impersonate",
      "impersonation",
      "user",
      "auth",
      "laravel-package",
      "laravel-plugin"
    ],
    "require": {
      "php": "^8.1",
      "laravel/framework": ">=5.8",
      "tymon/jwt-auth": "v2.0.0"        
    },
    "license": "MIT",
    "authors": [
      {
        "name": "rickycezar",
        "email": "barros.henrique.c@gmail.com"
      }
    ],
    "autoload": {
      "psr-4": {
        "Rickycezar\\Impersonate\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Rickycezar\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
